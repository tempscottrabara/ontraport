{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://www1.moon-ray.com/v2.4/autosauce_pthrough.php?key=ruVerU26"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Sales"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Setup"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Shipping"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Label + Packing Slips"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "add an email address"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "emails_email"
      },
      "text": "testEmail@testEmail.com"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.submit.corner"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//label[2]/input"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='feq_hour']/*[. = 'label=4']"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='meridian']/*[. = 'label=PM']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//fieldset[3]/fieldset/div/label[2]/input"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "contact_name"
      },
      "text": "testName"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "contact_email"
      },
      "text": "testEmail@testEmail.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "contact_phone"
      },
      "text": "testPhone"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "save"
      }
    }
  ]
}