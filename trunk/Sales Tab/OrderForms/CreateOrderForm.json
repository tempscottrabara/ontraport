{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://www1.moon-ray.com/v2.4/autosauce_pthrough.php?key=ruVerU26"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Sales"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Payment Gateways"
      }
    },
    {
      "type": "get",
      "url": "https://www1.moon-ray.com/v2.4/sales.php?action=product_manager&show=1"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Order Forms"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "action_btn_New_Order_Form"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "order_form_name"
      },
      "text": "Your Dreams"
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "(//a[contains(text(),'Add a Product')])[2]"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='productsWidget_id']/*[. = 'label=Your Dreams']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div.tipsWrapper.col"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "(//input[@value='save'])[2]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Add a Payment Gateway"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='cart_form_payment_gateway']/*[. = 'label=New Dummy Gateway']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "(//input[@value='save'])[6]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "save"
      }
    }
  ]
}