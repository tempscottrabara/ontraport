{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www1.moon-ray.com/v2.4/autosauce_pthrough.php?key=ruVerU26"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Admin"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Field Editor"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Add a New Field"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "input.inpt_text.standard_input"
      },
      "text": "NEWFIELD1"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='field_0']/div/table/tbody/tr/td[2]/span/select/*[.='Long Text']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='field_0']/div/table/tbody/tr/td[3]/img[2]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Add a New Field"
      }
    },
    {
      "type": "pause",
      "waitTime": "1000"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "input.inpt_text.standard_input"
      },
      "text": "NEWFIELD2"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='field_0']/div/table/tbody/tr/td[2]/span/select/*[.='List Selection']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "New Option"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "css selector",
        "value": "div.menuitem > input.inpt_text.standard_input"
      },
      "text": "OPTION1"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "New Option"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "(//input[@type='TEXT'])[3]"
      },
      "text": "OPTION2"
    },
    {
      "type": "pause",
      "waitTime": "1000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='field_0']/div/table/tbody/tr/td[3]/img[2]"
      }
    }
  ]
}