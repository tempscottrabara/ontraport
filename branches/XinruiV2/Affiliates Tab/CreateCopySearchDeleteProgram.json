{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www1.moon-ray.com/v2.4/autosauce_pthrough.php?key=ruVerU26"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Affiliates"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "action_btn_Create_a_new_Program"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "affiliate_program_name"
      },
      "text": "beef"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "save"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//input[@type='TEXT']"
      },
      "text": "beef\n"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "(//a[contains(text(),'Delete Form')])[3]"
      }
    },
    {
      "type": "assertAlertText",
      "text": "Are you sure you want to delete this Affiliate Program? All links between commission and the affiliates will be severed"
    }
  ]
}