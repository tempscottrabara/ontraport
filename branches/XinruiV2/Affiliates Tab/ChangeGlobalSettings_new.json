{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://app.ontraport.com/#!/affiliate/settings"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div/div/div/div[1]/div/div/button"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div/div/div/div[1]/div/div/*/*/*/*/*/*[.='Paypal']"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div/div/div/div[2]/div/div/input"
      },
      "text": "75"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div/div/div/div[3]/div/div/button"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div/div/div/div[3]/div/div/*/*/*/*/*/*[.='First Referrer']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div/div/div/div[4]/div/div/button"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[4]/div[1]/div/div/div/div[4]/div/div/*/*/*/*/*/*[.='30 days']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='ussr-chrome-panel-pane']/div[3]/div/div[4]/button[1]"
      }
    }
  ]
}