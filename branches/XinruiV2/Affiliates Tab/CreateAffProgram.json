{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www1.moon-ray.com/v2.4/autosauce_pthrough.php?key=ruVerU26"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Affiliates"
      }
    },
    {
      "type": "waitForElementPresent",
      "locator": {
        "type": "id",
        "value": "action_btn_Create_a_new_Program"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "action_btn_Create_a_new_Program"
      }
    },
    {
      "type": "waitForElementPresent",
      "locator": {
        "type": "id",
        "value": "affiliate_program_name"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "affiliate_program_name"
      },
      "text": "SEL IDE TEST"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "fieldset.last > label.inputGroupLabel > span.checkbox.fieldToggler > input.auto.disabled"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "affiliate_credit_after_first_sale_number"
      },
      "text": "15"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "affiliate_program_notify_on_commision"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='content']/div/div[1]/form/fieldset[4]/fieldset[4]/select/*[.='Placeholder']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "add a new product"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='com_product_id']/*[. = 'qqq']"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "com_level1"
      },
      "text": "2"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "com_level2"
      },
      "text": "18"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//input[@value='save']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "save"
      }
    }
  ]
}