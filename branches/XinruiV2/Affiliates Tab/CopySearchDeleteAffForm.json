{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www1.moon-ray.com/v2.4/autosauce_pthrough.php?key=ruVerU26"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Affiliates"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='item_container_div2']/table/tbody/tr[2]/td[1]/a[3]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.inpt_button.standard_input"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='item_container_div2']/table/thead/tr/td/table/tbody/tr/td[3]/table/tbody/tr[1]/td/input"
      },
      "text": "copy"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='item_container_div2']/table/tbody/tr[2]/td[1]/a[4]"
      }
    },
    {
      "type": "assertAlertText",
      "text": "Are you sure you want to delete this order form?"
    }
  ]
}