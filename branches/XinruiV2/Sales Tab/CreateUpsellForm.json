{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://www1.moon-ray.com/v2.4/autosauce_pthrough.php?key=ruVerU26"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Sales"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Forms"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Upsell Forms"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "action_btn_New_Upsell_Form"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "upsell_form_name"
      },
      "text": "selenium upsell form"
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='upsell_form_product']/*[. = 'index=0']"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "upsell_form_shipping_cost"
      },
      "text": "15"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "name",
        "value": "save"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Delete Form"
      }
    }
  ]
}