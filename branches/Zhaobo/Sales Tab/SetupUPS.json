{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://www1.moon-ray.com/v2.4/autosauce_pthrough.php?key=ruVerU26"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Sales"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Shipping"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "UPS Setup"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "license"
      },
      "text": "test key"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "username"
      },
      "text": "test account name"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "test account password"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "accountNumber"
      },
      "text": "test account number"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='PickupType']/*[. = 'label=Daily Pickup']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "option[value=\"01\"]"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "toCountryCode"
      },
      "text": "test country"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='verifyAddress']/*[. = 'label=validate']"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "insuredCurrency"
      },
      "text": "test currency"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "shipmentDescription"
      },
      "text": "test description"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "packageDescription"
      },
      "text": "test description"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='packagingType']/*[. = 'label=25KG Box']"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='returnCode']/*[. = 'label=UPS Print Return Label (PRL)']"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='referenceCode']/*[. = 'label=02']"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "//*[@id='residentialAddressIndicator']/*[. = 'label=Residential']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    }
  ]
}