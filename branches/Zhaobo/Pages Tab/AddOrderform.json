{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://www1.moon-ray.com/http://www1.moon-ray.com/v2.4/autosauce_pthrough.php?key=ruVerU26"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Pages"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "action_btn_New_Landing_Page"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='bodydiv']/span/div[2]/center/table/tbody/tr/td[1]/div"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "div.side_prop_label"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='bodydiv']/span/div[2]/center/div[6]/table/tbody/tr/td[2]"
      }
    },
    {
      "type": "pause",
      "waitTime": "2000"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='property_cell']/div/div/span/table/tbody/tr/td[1]/div/select/*[. = 'AutoSauce Test']"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//input[@type='TEXT']"
      },
      "text": "Your Dreams Landing Page"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.inpt_button.standard_input"
      }
    }
  ]
}