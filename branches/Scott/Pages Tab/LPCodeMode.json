{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://www1.moon-ray.com/v2.4/autosauce_pthrough.php?key=ruVerU26"
    },
    {
      "type": "get",
      "url": "https://www1.moon-ray.com/v2.4/contact_manager.php"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Pages"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "action_btn_New_Landing_Page"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='bodydiv']/span/div[2]/center/table/tbody/tr/td[2]/div"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//input[@type='TEXT']"
      },
      "text": "code mode"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "(//input[@type='TEXT'])[2]"
      },
      "text": "code mode"
    },
    {
      "type": "pause",
      "waitTime": "5000"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='property_cell']/table/tbody/tr/td[1]/div/table/tbody/tr/td[2]/select/*[. = 'First Name']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//input[@value='Insert']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.inpt_button.standard_input"
      }
    }
  ]
}