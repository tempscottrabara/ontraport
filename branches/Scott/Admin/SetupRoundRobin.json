    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Admin"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Lead Routing"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
