      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "rout_name"
      },
      "text": "Router Name"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='member_list']/*[.='Saucelabs Do Not Touch']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//input[@onclick='add_staff()']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//input[@onclick='check_router()']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Add Weighted Random Router"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "rout_name"
      },
      "text": "Router Name"
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": ".//*[@id='member_list']/*[.='Saucelabs Do Not Touch']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//input[@onclick='add_staff()']"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//input[@value='0']"
      },
      "text": "100"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//input[@value='Create Router']"
      }
    }
  ]
